{
  "name": "clinical-toolkit",
  "version": "0.2.0",
  "description": "Agent skill framework for clinical mental health screening, assessment interpretation, and documentation support",
  "author": "rhavekost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhavekost/clinical-toolkit.git"
  },
  "scripts": {
    "generate:manifests": "node scripts/generate-manifests.js",
    "generate:manifests:dry-run": "node scripts/generate-manifests.js --dry-run",
    "generate:claude": "node scripts/generate-manifests.js --platform=claude",
